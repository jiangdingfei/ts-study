{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "semicolon": false,
        "no-console": false,
        "interface-name": [true, "never-prefix"],
        "max-classes-per-file":false,
        "no-empty": [false],
        "no-empty-interface": [false],
        "interface-over-type-literal": [false],
        "variable-name": [false],
        "one-line": [false],
        "forin": [false],
        "no-shadowed-variable": [false],
        "ban-types": [false]
    },
    "rulesDirectory": []
}